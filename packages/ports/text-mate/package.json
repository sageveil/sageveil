{
  "name": "@sageveil/text-mate",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node tools/scripts/build-port.mjs {projectRoot}"
        },
        "outputs": [
          "{workspaceRoot}/dist/ports"
        ]
      },
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "command": "node tools/scripts/clean-port.mjs {projectRoot}"
        }
      }
    }
  }
}
