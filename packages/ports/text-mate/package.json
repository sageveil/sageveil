{
  "name": "@sageveil/text-mate",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "sageveil": {
    "displayName": "TextMate",
    "description": "Sageveil syntax tones for the TextMate editor.",
    "tags": [
      "editor"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/text-mate"
        ]
      },
      "clean": {}
    }
  }
}
