{
  "name": "@sageveil/nvim",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "sageveil": {
    "displayName": "Neovim",
    "description": "Sageveil highlights tuned for Neovim editing focus.",
    "tags": ["editor"]
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/nvim"
        ]
      },
      "clean": {}
    }
  }
}
