{
  "name": "@sageveil/k9s",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "sageveil": {
    "displayName": "K9s",
    "description": "Readable cluster views with Sageveil accents for k9s.",
    "tags": ["cli"]
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/k9s"
        ]
      },
      "clean": {}
    }
  }
}
