{
  "name": "@sageveil/alacritty",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "sageveil": {
    "displayName": "Alacritty",
    "description": "A focused, low-glare palette tuned for the Alacritty terminal.",
    "tags": ["terminal", "emulator"]
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/alacritty"
        ]
      },
      "clean": {}
    }
  }
}
