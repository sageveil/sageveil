{
  "name": "@sageveil/ghostty",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "sageveil": {
    "displayName": "Ghostty",
    "description": "Sageveil colors tuned for Ghostty's terminal rendering.",
    "tags": [
      "terminal",
      "emulator"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/ghostty"
        ]
      },
      "clean": {}
    }
  }
}
