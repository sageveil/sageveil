{
  "name": "@sageveil/glamour",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@sageveil/templater": "workspace:*"
  },
  "nx": {
    "targets": {
      "generate": {
        "outputs": [
          "{workspaceRoot}/dist/ports/glamour"
        ]
      },
      "clean": {}
    }
  },
  "sageveil": {
    "displayName": "Glamour",
    "description": "Markdown rendering with sageveil tones for Glamour.",
    "tags": [
      "cli"
    ]
  }
}
